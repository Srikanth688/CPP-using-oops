#include<iostream>
using namespace std;
int power(int m,int n=2)
{
   double result=1;
   for(int i=0;i<n;i++)
   {
       result*=m;
       
   }
   return result;
}
int main()
{
    cout<<"the values of power"<<endl;
    cout<<"the default value:"<<power(3)<<endl;
    cout<<"the value passed the function aruguments:"<<power(3,3);
    return 0;
}







